  "version": "3.0.0",
  "type": "craft-plugin",
  "require": {
    "craftcms/cms": "^5.0.0",
    "league/glide": "^3.0.0",
    "league/flysystem": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "wishanddell\\glide\\": "src/"
    }
  },
  "extra": {
    "handle": "glide2",
    "name": "Glide Image Transforms",
    "developer": "Wish & Dell",
    "developerUrl": "https://www.wishanddell.co.uk"
  }
}